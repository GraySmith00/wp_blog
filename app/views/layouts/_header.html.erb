<!-- HEADER -->
<header class="site-header" role="banner">
<!--   NAVBAR -->
  <div class="navbar-wrapper">
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          
<!--           LOGO PlACEMENT REPLACE "LOGO IMAGE W <img src=""> -->
          <a class="navbar-brand" href="/"><%= image_tag "logo.png" %></a>
        </div>
<!--           NAVBAR COLLAPSE -->
          <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
              <li class="active"><%= link_to "Home", root_path %></li>
              <li><%= link_to "Blog", posts_path %></li>
              <li><%= link_to "Resources", pages_resources_path %></li>
              <li><%= link_to "Contact", pages_contact_path %></li>
              <% if current_user %>
              <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
              <% else %>
              <li><%= link_to "Log In", new_user_session_path, method: :get %></li>
              <li><%= link_to "Sign Up", new_user_registration_path, method: :get %></li>
              <% end %>
            </ul>            
          </div>
          
        
      </div>
    </div>
  </div>
</header>